;! name: FIBO
;! entry: start
;! deps: display_number, newline
;! abi: os
;! bss: auto
;! align: 20

; .bss + 0: x
; .bss + 1: y

start:
  LDI 1
  STA .bss
  LDI 0
  STA .bss + 1
  STA .arg1
  JSR @display_number
  JSR @newline
add:
  LDA .bss
  ADD .bss + 1
  STA .arg1
  STA .bss
  JSR @display_number
  JSR @newline
  CPI 30
  JPC done
  ADD .bss + 1
  STA .arg1
  STA .bss + 1
  JSR @display_number
  JSR @newline
  CPI 30
  JPC done
  JMP add

done:
  RET