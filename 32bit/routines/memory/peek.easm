;! name: PEEK
;! entry: peek
;! deps: display_number, newline, parse_number
;! abi: argv
;! bss: none
;! align: 1
;! callable: yes

peek:
  ; argc at [__argv_base]
  LDA .__argv_base
  CPI 0
  JPZ pk_end
  LDA .__argv_base + 1      ; load pointer at [__argv_base+1]
  PHA
  JSR @parse_number
  LAP                     ; read number at mem[A]
  JSR @display_number
  JSR @newline
pk_end:
  RET
