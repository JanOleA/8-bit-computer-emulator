;! name: TGETMNEMONIC
;! entry: main
;! deps: get_mnemonic, display_number, newline
;! abi: os
;! bss: none
;! align: 4

main:
  ; require at least 1 argument
  LDI .argv_base
  PHA
  LAS
  CPI 1
  JPZ test_go
  RET

test_go:
  ; first arg -> string to compare
  LDI .argv_base
  ADI 1
  PHA
  LAS
  STA .arg1           ; res2 = pointer to string
  JSR @get_mnemonic
  LDA .res1
  STA .arg1
  JSR @display_number
  JSR @newline
  RET