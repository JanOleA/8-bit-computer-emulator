;! name: TGETMNEMONIC
;! entry: main
;! deps: get_mnemonic, display_number, newline
;! abi: all
;! bss: none
;! align: 4
;! callable: yes

main:
  ; require at least 1 argument
  LDA .argv_base
  CPI 1
  JPZ test_go
  RET

test_go:
  ; first arg -> string to compare
  LDA .argv_base + 1
  PHA
  JSR @get_mnemonic
  PHA
  JSR @display_number
  JSR @newline
  RET