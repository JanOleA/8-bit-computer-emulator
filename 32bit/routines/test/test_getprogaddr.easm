;! name: TGETPROG
;! entry: main
;! deps: get_program_address, display_number, newline
;! abi: all
;! bss: none
;! align: 4

main:
  ; require at least 1 argument
  LDA .argv_base
  CPI 1
  JPZ test_go
  RET

test_go:
  ; first arg -> string to compare
  LDA .argv_base + 1
  STA .arg1
  JSR @get_program_address
  LDA .res1
  PHA
  JSR @display_number
  JSR @newline
  RET