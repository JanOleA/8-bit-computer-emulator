meta name = Counter
meta entry = main
meta abi = os
depends newline, multiply, display_number

const ZERO = 0
var counter
var counter2
var result
var result2

data digits = "0123456789"

func main:
    let counter = 5
    let counter2 = 2
    while counter:
        let counter -= 1
        call @multiply(counter, counter2) -> result
        call @display_number(result)
    call @newline()

    call test(7, 8) -> result, result2
    call @display_number(result)
    call @newline()
    call @display_number(result2)
    call @newline()

    call test2(10, 20) -> result
    call @display_number(result)
    call @newline()
    return

func test(input1, input2):
    let counter = 3 + input1
    let counter2 = 4 + input2

    return counter, counter2

func test2(t2_input1, t2_input2):
    let result = t2_input1 + t2_input2
    return result